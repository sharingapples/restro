{
  "name": "restro-server",
  "version": "0.1.0",
  "description": "Restro server",
  "main": "build/index.js",
  "scripts": {
    "test": "jest",
    "lint": "../../node_modules/.bin/eslint src",
    "clean": "../../node_modules/.bin/rimraf build",
    "copy": "../../node_modules/.bin/copy src/**/*.sql build/",
    "build": "../../node_modules/.bin/babel src -d build",
    "prepare": "npm run lint && npm run clean && npm run build && npm run copy",
    "server-mon": "nodemon --exec ../../node_modules/.bin/babel-node -- src/index.js",
    "upgrade-fudo": "../../node_modules/.bin/babel-node -- src/db/upgrade/index.js"
  },
  "author": "Ranjan Shrestha",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "app-node": "^0.2.5",
    "bcrypt": "^2.0.1",
    "express": "^4.16.3",
    "lru-cache": "^4.1.3",
    "nodemailer": "^4.6.4",
    "restro-common": "^0.1.0",
    "shocked": "0.2.12",
    "sqlite": "^2.9.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-jest": "^22.4.3",
    "babel-preset-import-export": "^1.0.2",
    "copy": "^0.3.2",
    "jest": "^22.4.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
